{
    "name": "risingsun/laravel-notification-channel-apn",
    "description": "Apple APN Push Notification Channel",
    "homepage": "https://github.com/gaoxu529/apn",
    "license": "MIT",
    "authors": [
        {
            "name": "gaoxu",
            "email": "gaoxu529@126.com"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "edamov/pushok": "^0.12",
        "illuminate/cache": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/config": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/events": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/notifications": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Apn\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Apn\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2-dev"
        },
        "laravel": {
            "providers": [
                "NotificationChannels\\Apn\\ApnServiceProvider"
            ]
        }
    }
}
